<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="com.example.cleve.biblio_tech"
    android:versionCode="1"
    android:versionName="1.0">

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"

        android:supportsRtl="true"
        android:theme="@android:style/Theme.Holo">

        <activity
            android:name="com.example.cleve.biblio_tech.ListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.cleve.biblio_tech.BookActivity"
            android:label="@string/title_activity_book"
            android:parentActivityName="com.example.cleve.biblio_tech.ListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.cleve.biblio_tech.ListActivity" />
        </activity>
        <activity
            android:name="com.example.cleve.biblio_tech.ScanActivity"
            android:label="@string/title_activity_scan"
            android:parentActivityName="com.example.cleve.biblio_tech.ListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.cleve.biblio_tech.ListActivity" />
        </activity>
    </application>

</manifest>
